<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">

<suite name ="W3Schools Test Script" parallel="false">

    <parameter name="useCloudEnv" value="false"/>
    <parameter name="cloudEnvName" value="browserstack"/>
    <parameter name="os" value="Windows"/>
    <parameter name="os_version" value="10"/>
    <parameter name="browserName" value="Chrome"/>
    <parameter name="browserVersion" value="84.0"/>
    <parameter name="url" value="https://www.w3schools.com/sql/sql_where.asp"></parameter>


    <test name = "W3Schools Sql Table">

        <classes>
            <class name="tabledata.ReadTableData"/>
        </classes>

    </test>

</suite>
